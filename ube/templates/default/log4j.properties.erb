log4j.rootLogger=WARN, A1
log4j.appender.A1=org.apache.log4j.ConsoleAppender
log4j.appender.A1.layout=org.apache.log4j.PatternLayout

# Print the date in ISO 8601 format
log4j.appender.A1.layout.ConversionPattern=%d [%t] %-5p %c - %m%n

# Print only messages of level WARN or above in the package com.foo.
log4j.logger.com.shodogg=DEBUG
log4j.logger.com.shodogg.ube.interceptor=WARN
#log4j.logger.com.shodogg.ube.service.jobrunner=TRACE

#log4j.logger.com.shodogg.ube.business.hpsecurity=TRACE
#log4j.logger.com.shodogg.ube.interceptor.security.hp=TRACE
log4j.logger.com.amazonaws.services.s3.AmazonS3Client=ERROR
